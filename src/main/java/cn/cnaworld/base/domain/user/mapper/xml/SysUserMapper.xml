<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cnaworld.base.domain.user.mapper.SysUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.cnaworld.base.domain.user.entity.SysUser">
        <id column="user_id" property="userId" />
        <result column="user_account" property="userAccount" />
        <result column="user_name" property="userName" />
        <result column="user_passworld" property="userPassworld" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, user_account, user_name, user_passworld
    </sql>

</mapper>
